  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "state management",
    "react state management"
  ],
  "homepage": "https://github.com/vivekmunde/recell",
  "peerDependencies": {
    "pusu": "^1.2.2",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^18.2.33",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fast-deep-equal": "^3.1.3",
    "jest": "^24.8.0",
    "pusu": "^1.2.2",
    "react": "^16.14.0",
